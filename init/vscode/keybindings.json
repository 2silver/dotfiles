// Platzieren Sie Ihre Tastenzuordnungen in dieser Datei, um die Standardwerte zu Ã¼berschreiben.
[
    {
        "key": "cmd+y",
        "command": "redo",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "shift+cmd+z",
        "command": "-redo",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "f5",
        "command": "editor.action.sortLinesAscending"
    },
    {
        "key": "f4",
        "command": "editor.action.sortLinesDescending"
    }
]